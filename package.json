{
  "name": "go-channel",
  "version": "0.0.0",
  "description": "",
  "main": "./obj/main.js",
  "types": "./obj/main.d.ts",
  "scripts": {
    "prepare": "npm run compile",
    "compile": "tsc",
    "test": "npm run lint && npm run spec-all",
    "docs": "typedoc --excludePrivate --excludeExternals --readme readme.markdown --out docs --theme default",
    "lint": "tslint 'src/**/*.ts'",
    "spec": "tape --require 'ts-node/register'",
    "spec-all": "npm run spec 'src/**/*.spec.ts'",
    "coverage": "nyc --report-dir report --reporter text-summary --reporter lcov --include 'src/**/*.ts' --exclude 'src/**/*.spec.ts' --extension '.ts' npm run spec-all"
  },
  "author": "Elmer Bulthuis",
  "license": "ISC",
  "devDependencies": {
    "@types/blue-tape": "^0.1.31",
    "@types/tape": "^4.2.30",
    "blue-tape": "^1.0.0",
    "nyc": "^11.1.0",
    "tape": "^4.8.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.4.2"
  },
  "dependencies": {
    "promise-u": "^1.2.0",
    "tslib": "^1.7.1"
  }
}
